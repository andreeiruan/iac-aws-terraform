{
  "version": "1",
  "environment": "test",
  "region": "us-east-1",
  "services": [
    {
      "name": "nginx-test",
      "majorVersion": "1",
      "fullVersion": "1.2.3",
      "localDockerImage": "nginx:latest",
      "instanceType": "t2.micro",
      "networking": {
        "protocol": "HTTPS",
        "healthCheckPath": "/",
        "desiredCount": "2",
        "minCount": "2",
        "maxCount": "3",
        "hostedZoneName": "dev.ezops.com.br",
        "subdomain": "nginx-test"
      },
      "canaryDeployConfig": {
        "interval": "10",
        "percent": "50",
        "cleanupTimeout": "10"
      }
    }
  ]
}
